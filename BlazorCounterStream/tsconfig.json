{

  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2015",
    "allowUmdGlobalAccess": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "moduleResolution": "Classic",
    "lib": [ "DOM", "ES2015", "DOM.Iterable" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot/lib"
  ],
  "typeAcquisition": {
    "enable": true,
    "include": [ "jquery" ]
  }
}

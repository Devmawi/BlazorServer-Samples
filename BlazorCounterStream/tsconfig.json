{
  
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2015",
    
    "allowUmdGlobalAccess": true,
    "isolatedModules": false,
    "lib": [ "DOM", "DOM.Iterable", "ES2015" ]
  },
  
  "typeAcquisition": {"enable": true},
  "exclude": [
    "node_modules"
  ]
}
